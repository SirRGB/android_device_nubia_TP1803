# Data files
/data/per_boot(/.*)?                                                 u:object_r:per_boot_file:s0

# Fingerprint
/dev/sunwave_fp                                                      u:object_r:vendor_fingerprint_device:s0

# HALs
/vendor/bin/hw/android\.hardware\.power-service\.TP1803-libperfmgr   u:object_r:hal_power_default_exec:s0

# Sensors
/sys/devices/virtual/light/light(/.*)?                               u:object_r:vendor_sysfs_sensors:s0
/sys/devices/virtual/proximity/proximity(/.*)?                       u:object_r:vendor_sysfs_sensors:s0

# UFS
/sys/devices(/platform)?/soc/[a-f0-9]+.ufshc/clkgate_enable          u:object_r:vendor_sysfs_scsi_host:s0

# smart charging/cutoff
/sys/class/power_supply/battery/charging_enabled                u:object_r:sysfs_device_info:s0
/sys/class/power_supply/battery/temp                            u:object_r:sysfs_device_info:s0

# system info
/sys/class/thermal/thermal_zone0/temp                         u:object_r:sysfs_device_info:s0
/sys/devices/virtual/graphics/fb0/measured_fps                u:object_r:sysfs_device_info:s0
